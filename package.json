{
  "name": "nicefox-auth",
  "version": "1.0.1",
  "description": "Auth middleware for nicefox.net apps - JWT verification and Express middleware",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "build:frontend": "npm run build --prefix frontend"
  },
  "keywords": [
    "auth",
    "jwt",
    "middleware",
    "express",
    "nicefox",
    "authentication"
  ],
  "author": "Conrad L",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/co-l/nicefox-auth.git"
  },
  "bugs": {
    "url": "https://github.com/co-l/nicefox-auth/issues"
  },
  "homepage": "https://github.com/co-l/nicefox-auth#readme",
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "concurrently": "^9.1.2",
    "typescript": "^5.0.0"
  }
}
